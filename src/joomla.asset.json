{
  "$schema": "https://developer.joomla.org/schemas/json-schema/web_assets.json",
  "name": "musalie",
  "version": "4.0.0",
  "description": "Musalie template",
  "license": "GPL-2.0+",
  "assets": [
	{
	  "name": "template.musalie.base",
	  "type": "style",
	  "uri": "template.css",
	  "dependencies": [
		  "template.musalie.header",
		  "template.musalie.footer",
		  "fontawesome"
	  ]
	},
	{
	  "name": "template.musalie.virtuemart",
	  "type": "style",
	  "uri": "virtuemart.css",
	  "dependencies": [
		  "template.musalie.base",
		  "template.musalie.account"
	  ]
	},
	{
	  "name": "template.musalie.header",
	  "type": "style",
	  "uri": "header.css",
	  "dependencies": [
		  "template.musalie.navigation"
	  ]
	},
	{
	  "name": "template.musalie.footer",
	  "type": "style",
	  "uri": "footer.css"
	},
	{
	  "name": "template.musalie.home",
	  "type": "style",
	  "uri": "home.css",
	  "dependencies": "template.musalie.base"
	},
	{
	  "name": "template.musalie.navigation",
	  "type": "style",
	  "uri": "navigation.css"
  	},
	{
	  "name": "fontawesome",
	  "type": "style",
	  "uri": "system/joomla-fontawesome.min.css"
	},
	{
	  "name": "template.musalie.account",
	  "type": "style",
	  "uri": "account.css",
	  "dependencies": "template.musalie.base"
	},
	{
	  "name": "template.musalie.article",
	  "type": "style",
	  "uri": "article.css",
	  "dependencies": ["template.musalie.base", "template.musalie.blog", "template.musalie.imageslider"]
	},
	{
	  "name": "template.musalie.blog",
	  "type": "style",
	  "uri": "blog.css",
	  "dependencies": "template.musalie.base"
	},
	{
	  "name": "template.musalie.imageslider",
	  "type": "style",
	  "uri": "dj-imageslider.css",
	  "dependencies": "template.musalie.base"
	}

]
